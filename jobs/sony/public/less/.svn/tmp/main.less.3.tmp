  display: none;
}

.holder-spinner .spinner {
   position:relative;
   font-size: 175px;
   width:1em;
   height:1em;
   float: left;
}

.holder-spinner .spinner.hidden {
  display: none;
}

.holder-spinner .spinner > .percent {
   position: absolute;
   top: 1.05em;
   left: 0;
   width: 3.33em;
   font-size: 0.3em;
   text-align:center;
}

.holder-spinner .spinner > #slice {
   position:absolute;
   width:1em;
   height:1em;
   clip:rect(0px,1em,1em,0.5em);
}

.holder-spinner .spinner > #slice.gt50 {
   clip:rect(auto, auto, auto, auto);
}

.holder-spinner .spinner > #slice > .pie {
   border: 0.1em solid #c0c0c0;
   position:absolute;
   width:0.8em; /* 1 - (2 * border width) */
   height:0.8em; /* 1 - (2 * border width) */
   clip:rect(0em,0.5em,1em,0em);
   -moz-border-radius:0.5em;
   -webkit-border-radius:0.5em;
   -ms-border-radius:0.5em;
   border-radius:0.5em;
}

.holder-spinner .spinner > #slice > .pie.fill {
   -moz-transform:rotate(180deg) !important;
   -webkit-transform:rotate(180deg) !important;
   -o-transform:rotate(180deg) !important;
   -ms-transform:rotate(180deg) !important;
   transform:rotate(180deg) !important;
}

.holder-spinner .spinner.fill > .percent {
   display: none;
}

.holder-spinner .spinner.fill > #slice > .pie {
   border: transparent;
   background-color: #c43f33;
   width:1em;
   height:1em;
}

